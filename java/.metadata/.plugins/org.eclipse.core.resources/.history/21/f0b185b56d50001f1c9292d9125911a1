package cardDealer.ksy;

import java.util.ArrayList;
import java.util.List;

public class Dealer {
	//딜러가 카드를 가지고 있다
	private List<Card>cards = new ArrayList<Card>();
	
	public Dealer() {
		prepareCard();
		
	}

	private void prepareCard() {
		for ( int i =1; i <=13; i++) {
			for(Kind kind : Kind ) {
				cards.add(new Card(i,))
			}
		}
	}
}
